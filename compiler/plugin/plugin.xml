<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="edu.jhu.cs.bsj.eclipse.editor.BSJEditor"
            default="true"
            extensions="bsj"
            id="edu.jhu.cs.bsj.eclipse.editor.BSJEditor"
            name="BSJ Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="edu.jhu.cs.bsj.eclipse.perspective.BSJPerspectiveFactory"
            id="edu.jhu.cs.bsj.eclipse.perspective.BSJPerspectiveFactory"
            name="BSJ">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="edu.jhu.cs.bsj.eclipse.preference.BSJPreferencePage"
            id="edu.jhu.cs.bsj.eclipse.preferences.BSJPreferencePage"
            name="BSJ">
      </page>
      <page
            category="edu.jhu.cs.bsj.eclipse.preferences.BSJPreferencePage"
            class="edu.jhu.cs.bsj.eclipse.preference.BuildPathPreferencePage"
            id="edu.jhu.cs.bsj.eclipse.preferences.BuildPathPreferencePage"
            name="Build Path">
      </page>
      <page
            category="edu.jhu.cs.bsj.eclipse.preferences.BSJPreferencePage"
            class="edu.jhu.cs.bsj.eclipse.preference.CompilerPreferencePage"
            id="edu.jhu.cs.bsj.eclipse.preferences.CompilerPreferencePage"
            name="Compiler">
      </page>
      <page
            category="edu.jhu.cs.bsj.eclipse.preferences.BSJPreferencePage"
            class="edu.jhu.cs.bsj.eclipse.preference.DebugPreferencePage"
            id="edu.jhu.cs.bsj.eclipse.preferences.DebugPreferencePage"
            name="Debug">
      </page>
      <page
            category="edu.jhu.cs.bsj.eclipse.preferences.BSJPreferencePage"
            class="edu.jhu.cs.bsj.eclipse.preference.EditorPreferencePage"
            id="edu.jhu.cs.bsj.eclipse.preferences.EditorPreferencePage"
            name="Editor">
      </page>
      <page
            category="edu.jhu.cs.bsj.eclipse.preferences.EditorPreferencePage"
            class="edu.jhu.cs.bsj.eclipse.preference.SyntaxColoringPreferencePage"
            id="edu.jhu.cs.bsj.eclipse.preference.SyntaxColoringPreferencePage"
            name="Syntax Coloring">
      </page>
      <page
            class="edu.jhu.cs.bsj.eclipse.plugin.preferences.SamplePreferencePage"
            id="edu.jhu.cs.bsj.eclipse.plugin.preferences.SamplePreferencePage"
            name="Sample Preferences">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="edu.jhu.cs.bsj.eclipse.preference.SyntaxColoringPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         id="edu.jhu.cs.bsj.eclipse.builder"
         name="BSJBuilder"
         point="org.eclipse.core.resources.builders">
      <builder
            hasNature="true">
         <run
               class="edu.jhu.cs.bsj.eclipse.builder.BSJBuilder">
         </run>
      </builder>
   </extension>
   <extension
         id="edu.jhu.cs.bsj.eclipse.marker"
         name="BSJMarker"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>
   <extension
         id="edu.jhu.cs.bsj.eclipse.nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="edu.jhu.cs.bsj.eclipse.builder.BSJNature">
         </run>
      </runtime>
      <builder
            id="edu.jhu.cs.bsj.eclipse.builder">
      </builder>
   </extension>
   
</plugin>
