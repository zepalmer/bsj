<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="edu.jhu.cs.bsj.plugin.eclipse.editor.BSJEditor"
            default="true"
            extensions="bsj"
            id="edu.jhu.bsj.plugin.eclipse.editor.bsjeditor"
            name="BSJ Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.editors.documentProviders">
      <provider
            class="org.eclipse.ui.editors.text.TextFileDocumentProvider"
            extensions="bsj"
            id="edu.jhu.bsj.plugin.eclipse.editor.documentprovider">
      </provider>
   </extension>
   <extension
         point="org.eclipse.core.filebuffers.documentSetup">
      <participant
            class="edu.jhu.cs.bsj.plugin.eclipse.editor.BSJDocumentSetupParticipant"
            extensions="bsj">
      </participant>
   </extension>

</plugin>
