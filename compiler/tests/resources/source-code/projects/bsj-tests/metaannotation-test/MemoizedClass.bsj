#import edu.jhu.cs.bsj.stdlib.metaannotations.*;

public class MemoizedClass
{
    public MemoizedClass()
    {
        
    }
    
    @@Memoized
    public Integer foo(int x, int y)
    {
        return (x+y)*2;
    }
    
    //@@Memoized
    public String bar(String s)
    {
        return "new" + s;
    }
}